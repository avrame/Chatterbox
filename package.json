{
  "name": "chatterbox",
  "version": "1.0.0",
  "description": "A Chat App",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run client\"",
    "postinstall": "cd client && npm install",
    "server:dev": "nodemon server.js",
    "server:prod": "node server.js",
    "client": "node start-client.js",
    "build": "node build-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avrame/Chatterbox.git"
  },
  "author": "Avram Eisner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avrame/Chatterbox/issues"
  },
  "homepage": "https://github.com/avrame/Chatterbox#readme",
  "dependencies": {
    "@blueprintjs/core": "3.18.1",
    "babel-core": "6.26.3",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "express-session": "1.16.2",
    "gravatar": "1.8.0",
    "mongoose": "5.7.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "5.0.1",
    "react-recaptcha": "2.3.10",
    "react-router-dom": "5.0.1",
    "slugify": "1.3.5",
    "ws": "7.1.2"
  },
  "devDependencies": {
    "concurrently": "4.1.2"
  }
}
